#include<stdio.h>
#include<conio.h>
int main(){
   int disp[2][3];
   int i, j;
   for(i=0; i<4; i++) {
      for(j=0;j<4;j++) {
         printf("Enter value for disp[%d][%d]:", i, j);
         scanf("%d", &disp[i][j]);
      }
   }
   printf("Two Dimensional array elements:\n");
   for(i=0; i<4; i++) {
      for(j=0;j<4;j++) {
         printf("%d ", disp[i][j]);
         if(j==3){
            printf("\n");
         }
      }
   }
   return 0;
}
